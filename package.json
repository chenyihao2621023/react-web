{
  "name": "react-babel-webpack-boilerplate",
  "version": "1.4.1",
  "description": "A boilerplate for React-Babel-Webpack project",
  "main": "app/main.js",
  "scripts": {
    "precommit": "npm run lint",
    "commitmsg": "npm run lint:msg",
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint",
    "build": "webpack",
    "start": "webpack-dev-server --color --progress --hot --content-base app",
    "start:mock": "babel-node tools/mock.js",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "6.x",
    "better-scroll": "^0.1.7",
    "es6-promise": "^4.0.5",
    "history": "^4.6.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.3",
    "rc-animate": "^2.3.3",
    "react": "15.x",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "15.x",
    "react-mixin": "^3.0.5",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "6.x",
    "babel-eslint": "7.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "1.x",
    "copy-webpack-plugin": "latest",
    "css-loader": "~0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "3.x",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "latest",
    "file-loader": "^0.10.1",
    "husky": "^0.12.0",
    "json-server": "^0.9.5",
    "open-browser-webpack-plugin": "0.0.3",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "server-destroy": "^1.0.1",
    "style-loader": "~0.13.1",
    "stylus-loader": "^2.5.0",
    "url-loader": "^0.5.8",
    "validate-commit-msg": "2.x",
    "webpack": "1.x",
    "webpack-dev-server": "1.x"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ruan Yifeng",
  "license": "MIT",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
